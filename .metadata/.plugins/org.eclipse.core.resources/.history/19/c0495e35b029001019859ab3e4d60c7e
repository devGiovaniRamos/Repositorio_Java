package exe001;

import java.util.Scanner;

public class exe {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int l, a, p, m, res;
		
		l = sc.nextInt();
		sc.nextLine();
		a = sc.nextInt();
		sc.nextLine();
		p = sc.nextInt();
		sc.nextLine();
		
		m = l * a;
		
		res = p * m;
		
		System.out.println("O terreno tem "+ m +" metros quadrados e"
				+ " custar√° "+ res +" reais.");
		

	}

}
